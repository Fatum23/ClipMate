{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main"],
	"permissions": [
		"path:default",
		"event:default",
		"window:default",
		"window:allow-start-dragging",
		"app:default",
		"image:default",
		"resources:default",
		"menu:default",
		"tray:default",
		"shell:allow-open",
		"global-shortcut:default",
		"global-shortcut:allow-register",
		"global-shortcut:allow-is-registered"
	]
}
